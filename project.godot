config_version=5

[application]

config/name="UISystem"
run/main_scene="res://UISystem/DefaultScene.tscn"
config/features=PackedStringArray("4.3", "C#", "GL Compatibility")

[autoload]

UiInstaller="*res://UISystem/UIInstaller.tscn"

[display]

